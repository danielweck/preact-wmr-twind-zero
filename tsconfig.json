{
	"compileOnSave": false,
	"compilerOptions": {
		"declaration": true,
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"strictNullChecks": true,
		"target": "ES2020",
		"module": "es2020",
		"moduleResolution": "nodenext",
		"allowSyntheticDefaultImports": true,
		"downlevelIteration": true,
		"skipLibCheck": true,
		"esModuleInterop": false,
		"importsNotUsedAsValues": "error",
		"preserveValueImports": false
	}
}
