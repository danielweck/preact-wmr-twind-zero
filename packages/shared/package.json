{
	"name": "@preact-wmr-twind-zero/shared",
	"version": "0.0.0",
	"description": "Preact WMR Twind 'zero runtime' monorepo - shared code",
	"homepage": "https://github.com/danielweck/preact-wmr-twind-zero/blob/main/README.md",
	"repository": "github:danielweck/preact-wmr-twind-zero/",
	"bugs": "https://github.com/danielweck/preact-wmr-twind-zero/issues",
	"author": "Daniel Weck (https://github.com/danielweck)",
	"keywords": [
		"Preact",
		"WMR",
		"Twind",
		"SSG",
		"Server Side Pre-rendering",
		"Critical CSS Stylesheet",
		"Performance",
		"Web Vitals"
	],
	"license": "BSD3",
	"type": "module",
	"module": "./dist/esm/index.js",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./*": {
			"import": "./dist/esm/*",
			"require": "./dist/cjs/*"
		},
		"./sub": {
			"import": "./dist/esm/sub/other.js",
			"require": "./dist/cjs/sub/other.js"
		},
		"./sub/*": {
			"import": "./dist/esm/sub/*",
			"require": "./dist/cjs/sub/*"
		}
	},
	"engines": {
		"node": ">=16",
		"pnpm": ">=6"
	},
	"scripts": {
		"clean": "rimraf \"./dist\" && rimraf \"./.turbo\"",
		"start": "pnpm build",
		"build": "pnpm clean && pnpm typescript",
		"typescript": "tsc --version && pnpm typescript-esm",
		"typescript-esm": "tsc --project tsconfig.build.json",
		"typescript-cjs": "tsc --project tsconfig.build-cjs.json"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"tag": "latest",
		"access": "public",
		"directory": "dist"
	}
}
