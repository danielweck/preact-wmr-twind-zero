{
	"$schema": "https://turborepo.org/schema.json",
	"baseBranch": "origin/main",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"esbuild": {
			"dependsOn": ["^esbuild"],
			"outputs": ["dist/**"]
		},
		"typescript": {
			"dependsOn": ["^typescript"],
			"outputs": ["dist/**"]
		},
		"build-github": {
			"dependsOn": ["^build", "^build-github"],
			"outputs": ["dist/**"]
		},
		"dev": {
			"cache": false
		}
	}
}
