{
	"type": "module",
	"engines": {
		"node": ">=16",
		"npm": ">=8",
		"pnpm": ">=6",
		"yarn": ">=1"
	},
	"scripts": {
		"clean": "rimraf \"./dist\" && rimraf \"./.cache\" && rimraf \"./.tmp\"",
		"prestart": "npm run clean",
		"start": "cross-env DEBUG=* wmr",
		"prebuild": "npm run clean",
		"build": "cross-env DEBUG=* wmr build --prerender",
		"postbuild": "node \"./wmr-post-prerender-twind.mjs\"",
		"prebuild-github": "npm run prebuild",
		"build-github": "cross-env DEBUG=* BUILD_FOR_GITHUB=1 wmr build --prerender",
		"postbuild-github": "npm run postbuild",
		"prebuild-viz": "npm run prebuild",
		"build-viz": "cross-env DEBUG=* wmr build --prerender --visualize",
		"postbuild-viz": "npm run postbuild",
		"serve": "cross-env DEBUG=* wmr serve",
		"serve-http2": "cross-env DEBUG=* wmr serve --http2",
		"lint:eslint": "eslint \"./**/*.{ts,tsx,js,tsx,mjs}\"",
		"lint:editorconfig": "eclint check '**/*' '!.tmp/**/*' '!package-lock.json' '!package.json' '!yarn.lock' '!pnpm-lock.yaml' '!.cache/**/*' '!.vscode/**/*' '!.git/**/*' '!node_modules/**/*' '!.github/**/*' '!.history/**/*' '!dist/**/*'",
		"lint-prettier": "prettier --check --no-write --loglevel log \"./\"",
		"lint": "npm run lint:editorconfig && npm run lint:eslint && npm run lint-prettier && npm run typescript",
		"eslint-fix": "eslint --fix \"./**/*.{js,ts,js,tsx,mjs}\"",
		"prettier-fix": "prettier --write \"./**/*.{css,html,mjs,js,ts,jsx,tsx,json,md,yml,svg,xml}\"",
		"lint-fix": "npm run eslint-fix && npm run prettier-fix",
		"typescript": "tsc --project tsconfig.json"
	},
	"dependencies": {
		"preact": "^10.6.4",
		"preact-iso": "^2.3.0"
	},
	"devDependencies": {
		"@twind/preset-autoprefix": "^1.0.0-next.17",
		"@twind/preset-ext": "^1.0.0-next.17",
		"@twind/preset-tailwind": "^1.0.0-next.17",
		"@twind/preset-tailwind-forms": "^1.0.0-next.17",
		"@twind/typescript-plugin": "^0.3.4",
		"@types/node": "^16.11.17",
		"@typescript-eslint/eslint-plugin": "^5.10.0",
		"@typescript-eslint/parser": "^5.10.0",
		"cpy-cli": "^3.1.1",
		"cross-env": "^7.0.3",
		"eclint": "^2.8.1",
		"eslint": "^8.7.0",
		"eslint-config-preact": "^1.3.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"kolorist": "^1.5.1",
		"prettier": "^2.5.1",
		"rimraf": "^3.0.2",
		"rollup": "^2.64.0",
		"twind": "^1.0.0-next.17",
		"typescript": "^4.5.5",
		"wmr": "^3.7.2"
	}
}
