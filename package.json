{
	"type": "module",
	"engines": {
		"node": ">=16",
		"pnpm": ">=6"
	},
	"packageManager": "pnpm@6.29.1",
	"scripts": {
		"prepare": "husky install",
		"clean": "rimraf \"./dist\" && rimraf \"./.cache\" && rimraf \"./.tmp\"",
		"start": "npm run clean && cross-env DEBUG=* wmr",
		"build-no-prerender": "npm run clean && cross-env DEBUG=* wmr build",
		"build": "npm run clean && cross-env DEBUG=* wmr build --prerender && npm run afterbuild",
		"build-repeat-if-fail": "(npm run build || npm run build || npm run build || npm run build || npm run build || npm run build || npm run build || npm run build || npm run build || npm run build || exit 0) || echo OK",
		"afterbuild": "node \"./wmr-post-prerender-twind.mjs\"",
		"build-github": "npm run clean && cross-env DEBUG=* BUILD_FOR_GITHUB=1 wmr build --prerender && npm run afterbuild",
		"build-github-repeat-if-fail": "(npm run build-github || npm run build-github || npm run build-github || npm run build-github || npm run build-github || npm run build-github || npm run build-github || npm run build-github || npm run build-github || npm run build-github || exit 0) || echo OK",
		"build-viz": "npm run clean && cross-env DEBUG=* wmr build --prerender --visualize && npm run afterbuild",
		"serve": "cross-env DEBUG=* wmr serve",
		"serve-http2": "cross-env DEBUG=* wmr serve --http2",
		"lint:eslint": "eslint \"./**/*.{ts,tsx,js,tsx,mjs,cjs}\"",
		"lint:editorconfig": "eclint check '**/*' '!.husky/**/*' '!.tmp/**/*' '!package-lock.json' '!package.json' '!yarn.lock' '!pnpm-lock.yaml' '!.cache/**/*' '!.vscode/**/*' '!.git/**/*' '!node_modules/**/*' '!.github/**/*' '!.history/**/*' '!dist/**/*'",
		"lint-prettier": "prettier --check --no-write --loglevel log \"./\"",
		"lint-stylelint": "stylelint \"./public/**/*.{html,htm,css,ts,tsx,js,tsx,mjs,cjs}\"",
		"lint": "npm run lint:editorconfig && npm run lint:eslint && npm run lint-stylelint && npm run lint-prettier && npm run typescript",
		"eslint-fix": "cross-env TIMING=1 eslint --cache --fix \"./**/*.{ts,tsx,js,tsx,mjs,cjs}\"",
		"prettier-fix": "prettier --ignore-unknown --write \"./**/!(pnpm-lock.yaml)*.{html,htm,css,json,md,yml,yaml,svg,xml,ts,tsx,js,tsx,mjs,cjs}\"",
		"stylelint-fix": "stylelint --fix \"./public/**/*.{html,htm,css,ts,tsx,js,tsx,mjs,cjs}\"",
		"lint-fix": "npm run eslint-fix && npm run prettier-fix && npm run stylelint-fix",
		"typescript": "tsc --project tsconfig.json"
	},
	"lint-staged": {
		"*.{ts,tsx,js,tsx,mjs,cjs}": [
			"pnpm eslint-fix"
		],
		"!(pnpm-lock.yaml)*.{html,htm,css,json,md,yml,yaml,svg,xml,ts,tsx,js,tsx,mjs,cjs}": [
			"pnpm prettier-fix"
		],
		"*.{html,htm,css,ts,tsx,js,tsx,mjs,cjs}": [
			"pnpm stylelint-fix"
		]
	},
	"dependencies": {
		"preact": "^10.6.5",
		"preact-iso": "^2.3.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.1.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@parcel/css": "^1.2.0",
		"@twind/preset-autoprefix": "^1.0.0-next.35",
		"@twind/preset-ext": "^1.0.0-next.35",
		"@twind/preset-tailwind": "^1.0.0-next.35",
		"@twind/preset-tailwind-forms": "^1.0.0-next.35",
		"@twind/typescript-plugin": "^0.3.5",
		"@types/node": "^16.11.22",
		"@typescript-eslint/eslint-plugin": "^5.11.0",
		"@typescript-eslint/parser": "^5.11.0",
		"browserslist": "^4.19.1",
		"cpy-cli": "^3.1.1",
		"cross-env": "^7.0.3",
		"eclint": "^2.8.1",
		"eslint": "^8.8.0",
		"eslint-config-preact": "^1.3.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^7.0.4",
		"kolorist": "^1.5.1",
		"lint-staged": "^12.3.3",
		"postcss-html": "^1.3.0",
		"postcss-jsx": "^0.36.4",
		"postcss-syntax": "^0.36.2",
		"prettier": "^2.5.1",
		"rimraf": "^3.0.2",
		"rollup": "^2.67.2",
		"stylelint": "^14.4.0",
		"stylelint-config-html": "^1.0.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard": "^25.0.0",
		"twind": "^1.0.0-next.35",
		"typescript": "^4.5.5",
		"wmr": "^3.7.2"
	}
}
